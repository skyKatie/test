#include<iostream>
using namespace std;
int add(int n,int m)
{
 if(n==1)
 return 1;
 if(n==2)
 return 2;
 if(n>2&&m==1)
 {
  return 2*add(n-1,m); 
 }
 if(n>2&&m==2)
 {
  return add(n-1,m)+add(n-2,m);
 }
 } 
 int main()
 {
  int n,m;
  cout<<"请输入正整数和m:"<<endl;
  cin>>n>>m;
  cout<<"有"<<add(n,m)<<"种方案"<<endl;
  return 0;
  
  
  
 }
